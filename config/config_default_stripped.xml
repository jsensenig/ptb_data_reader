<config>
  <Hardware>
    <PulseWidth>2</PulseWidth>
  </Hardware>
  <DataBuffer>
    <DaqHost>localhost</DaqHost>
    <DaqPort>8992</DaqPort>
    <RollOver>80</RollOver>
    <MicroSliceDuration>7</MicroSliceDuration>
  </DataBuffer>
  <ChannelMask>
    <BSU>0x1FFFFFFFFFFFF</BSU>
    <TSU>0xFFFFFFFFFFFF</TSU>
  </ChannelMask>
  <MuonTriggers num_triggers="4">
    <TriggerWindow>0x3</TriggerWindow>
    <LockdownWindow>0xD</LockdownWindow>
    <TriggerMask id="A" mask="1000">
      <group1>
        <Logic>0x2</Logic>
        <BSU>0xFFFF</BSU>
        <TSU>0x0</TSU>
      </group1>
      <group2>
        <Logic>0x2</Logic>
        <BSU>0x7F7E000000</BSU>
        <TSU>0x0</TSU>
      </group2>
      <ExtLogic>0x0</ExtLogic>
      <Prescale>0x0</Prescale>
    </TriggerMask>
    <TriggerMask id="B" mask="0100">
      <group1>
        <Logic>0x1</Logic>
        <BSU>0x0</BSU>
        <TSU>0x3F000000</TSU>
      </group1>
      <group2>
        <Logic>0x1</Logic>
        <BSU>0x0</BSU>
        <TSU>0xFC0000000</TSU>
      </group2>
      <ExtLogic>0x0</ExtLogic>
      <Prescale>0x0</Prescale>
    </TriggerMask>
    <TriggerMask id="C" mask="0010">
      <group1>
        <Logic>0x1</Logic>
        <BSU>0x0</BSU>
        <TSU>0xFC0000000000</TSU>
      </group1>
      <group2>
        <Logic>0x1</Logic>
        <BSU>0x0</BSU>
        <TSU>0x3F000000000</TSU>
      </group2>
      <ExtLogic>0x0</ExtLogic>
      <Prescale>0x0</Prescale>
    </TriggerMask>
    <TriggerMask id="D" mask="0001">
      <group1>
        <Logic>0x1</Logic>
        <BSU>0x0</BSU>
        <TSU>0x7FE00</TSU>
      </group1>
      <group2>
        <Logic>0x1</Logic>
        <BSU>0x0</BSU>
        <TSU>0x1FF</TSU>
      </group2>
      <ExtLogic>0x0</ExtLogic>
      <Prescale>0x0</Prescale>
    </TriggerMask>
  </MuonTriggers>
  <ExtTriggers>
    <Mask>0xF</Mask>
    <EchoTriggers>false</EchoTriggers>
  </ExtTriggers>
  <Calibrations>
    <CalibrationMask id="C1" mask="1000">
      <enabled>false</enabled>
      <period>0xA</period>
    </CalibrationMask>
    <CalibrationMask id="C2" mask="0100">
      <enabled>false</enabled>
      <period>0xA</period>
    </CalibrationMask>
    <CalibrationMask id="C3" mask="0010">
      <enabled>false</enabled>
      <period>0xA</period>
    </CalibrationMask>
    <CalibrationMask id="C4" mask="0001">
      <enabled>false</enabled>
      <period>0xA</period>
    </CalibrationMask>
  </Calibrations>
</config>
